  color: #fff;
  text-align: center;
}
</style>
<style>
    .login-container {
  width: 400px;
  height: 500px;
  margin: 80px auto;
  border-radius: 20px;
  background-color: #fff;
  box-shadow: 0 0 20px 0 #ccc;
}

.form-header {
  background-color: #0a1751df;
  color: #fff;
  text-align: center;
  padding: 25px;
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
}

.form-header h2 {
  font-size: 20px;
  margin: 0;
  padding: 0;
}

.form-body {
  padding: 45px;
}

.form-group {
  margin-bottom: 25px;
}

.form-control {
  width: 100%;
  height: 40px;
  border-radius: 25px;
  border: none;
  padding: 0 20px;
  font-size: 18px;
  box-shadow: 0 0 10px 0 #ccc;
}

.btn-submit {
  width: 100%;
  height: 35px;
  border-radius: 25px;
  border: none;
  background-color: #0a1751df;
  color: #fff;
  font-size: 15px;
  cursor: pointer;
  transition: all 0.3s ease-in-out;
}

.btn-submit:hover {
  background-color: #fff;
  color: #333;
  box-shadow: 0 0 20px 0 #ccc;
}

</style>
<style>
    .file-upload {
        display: inline-block;
        padding: 5px 10px;
        background-color: lightblue;
        color: white;
        border-radius: 4px;
        text-align: center;
        text-decoration: none;
        font-size: 16px;
        cursor: pointer;
        border: none;
    }
        .file-upload input[type="file"] {
            display: none;
        }
</style>
<style>
