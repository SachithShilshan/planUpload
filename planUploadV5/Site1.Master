<style type="text/css">
    .grid-view {
        font-family: Arial, sans-serif;
        font-size: 12px;
        width: 100%;
        border-collapse: collapse;
    }

        .grid-view th {
            background-color: #ddd;
            padding: 8px;
            border: 1px solid #999;
        }

        .grid-view td {
            padding: 8px;
            border: 1px solid #999;
        }
</style>
<style>
    header {
        background-color: #0a1751df;
        color: #fff;
        padding: 20px;
    }

        header nav ul {
            list-style: none;
            margin: 0;
            padding: 0;
            display: flex;
            justify-content: space-between;
        }

        header nav li {
            display: inline-block;
            margin-right: 20px;
        }

        header nav a {
            color: #fff;
            text-decoration: none;
        }
</style>
<style>
h4 {
